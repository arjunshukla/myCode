    if (buttonIndex == 0) {
        //Do something
        NSLog(@"Cancel");
    } else if (buttonIndex == 1) {
        //Do something else
        NSLog(@"OK");
        self.incidentTypeTextField.text = NULL;
        self.welltypeTextField.text= NULL;
        self.wellNameTextField.text = NULL;
        self.datetimeTextField.text = NULL;
        self.locationTextField.text=NULL;
        
    }
}

